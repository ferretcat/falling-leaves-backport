{
  "schemaVersion": 1,
  "id": "spring_to_life",
  "version": "${version}",
  "name": "Spring to Life",
  "description": "This mod ports some features from the \"Spring to Life\" feature drop (Minecraft 1.21.5) back to 1.21.1",
  "authors": [
    "LarsenR"
  ],
  "contact": {
    "homepage": "https://larsensmods.de/",
    "sources": "https://github.com/MC-Backports/spring_to_life"
  },
  "license": "GPL-3.0",
  "icon": "assets/spring_to_life/logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "de.larsensmods.stl_backport.fabric.SpringToLifeModFabric"
    ],
    "client": [
      "de.larsensmods.stl_backport.fabric.client.SpringToLifeModFabricClient"
    ]
  },
  "mixins": [
    "spring_to_life.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.14",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
